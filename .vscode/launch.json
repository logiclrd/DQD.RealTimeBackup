{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations":
	[
		{
			"type": "coreclr",
			"request": "launch",
			"name": "Start Initial Backup Then Exit",
			"program": "${workspaceFolder}/DeltaQ.RTB/bin/Debug/net8.0/DeltaQ.RTB.dll",
			"args": [ "/INITIALBACKUPTHENEXIT", "/VERBOSE" ],
		},
		{
			"type": "dotnet",
			"request": "launch",
			"name": "Start File Activity Trace",
			"projectPath": "${workspaceFolder}/DeltaQ.RTB.FileActivityTrace/DeltaQ.RTB.FileActivityTrace.csproj",
		},
		{
			"type": "coreclr",
			"request": "launch",
			"name": "Restore: List All Files",
			"program": "${workspaceFolder}/DeltaQ.RTB.Restore/bin/Debug/net8.0/DeltaQ.RTB.Restore.dll",
			"args": [ "/LISTALLFILES" ],
		},
		{
			"type": "coreclr",
			"request": "launch",
			"name": "Restore: Restore File",
			"program": "${workspaceFolder}/DeltaQ.RTB.Restore/bin/Debug/net8.0/DeltaQ.RTB.Restore.dll",
			"args": [ "/RESTOREFILE", "/DrBuffy/8-Cameo/Source/Cameo-Retimed.mkv" ],
		},
		{
			"type": "coreclr",
			"request": "launch",
			"name": "Restore: Restore Directory",
			"program": "${workspaceFolder}/DeltaQ.RTB.Restore/bin/Debug/net8.0/DeltaQ.RTB.Restore.dll",
			"args": [ "/RESTOREDIRECTORY", "/DrBuffy/8-Cameo/Source" ],
		},
	]
}