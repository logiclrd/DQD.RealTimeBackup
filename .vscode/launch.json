	"version": "0.2.0",
	"configurations":
	[
		{
			"type": "coreclr",
			"request": "launch",
			"name": "Start Initial Backup Then Exit",
			"program": "${workspaceFolder}/DeltaQ.RTB/bin/Debug/net8.0/DeltaQ.RTB.dll",
			"args": [ "/INITIALBACKUPTHENEXIT", "/VERBOSE" ],
		},
		{
			"type": "dotnet",
			"request": "launch",
			"name": "Start File Activity Trace",
			"projectPath": "${workspaceFolder}/DeltaQ.RTB.FileActivityTrace/DeltaQ.RTB.FileActivityTrace.csproj",
		}
	]
}